<h1>PrimarioX list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Matricula</th>
            <th>Actualizado</th>
            <th>Creado</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('primario_x_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.matricula }}</td>
            <td>{% if entity.actualizado %}{{ entity.actualizado|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.creado %}{{ entity.creado|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('primario_x_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('primario_x_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('primario_x_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
