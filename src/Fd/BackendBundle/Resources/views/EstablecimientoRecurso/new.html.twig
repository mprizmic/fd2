{% extends 'BackendBundle::layout.html.twig' %}
 
{% block main_content %}

<h1>Crear registro de nuevo</h1>


<form action="{{ path('establecimiento_recurso_create') }}" method="post" {{ form_enctype(form) }}>
    <div class="ancho_10">
        {{ form_widget(form) }}
    </div>
    <p>
        <button type="submit">Crear</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('establecimiento_recurso') }}">
            Volver a la lista
        </a>
    </li>
</ul>

{% endblock %}