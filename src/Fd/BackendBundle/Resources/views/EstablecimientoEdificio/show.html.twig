{% extends 'BackendBundle::layout.html.twig' %}
 
{% block title %}
{{ parent() }} - EstablecimientoEdificio
{% endblock %}
 
{% block main_content %}

<h1>EstablecimientoEdificio</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Cue_anexo</th>
            <td>{{ entity.cueanexo }}</td>
        </tr>
        <tr>
            <th>Nombre</th>
            <td>{{ entity.nombre }}</td>
        </tr>
        <tr>
            <th>Fecha_creacion</th>
            <td>{{ entity.fechacreacion|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Fecha_baja</th>
            <td>{{ entity.fechabaja|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Te1</th>
            <td>{{ entity.te1 }}</td>
        </tr>
        <tr>
            <th>Te2</th>
            <td>{{ entity.te2 }}</td>
        </tr>
        <tr>
            <th>Te3</th>
            <td>{{ entity.te3 }}</td>
        </tr>
        <tr>
            <th>Email1</th>
            <td>{{ entity.email1 }}</td>
        </tr>
        <tr>
            <th>Email2</th>
            <td>{{ entity.email2 }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('backend_establecimiento_edificio') }}">
            Volver a la lista
        </a>
    </li>
    <li>
        <a href="{{ path('backend_establecimiento_edificio_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>
        <form action="{{ path('backend_establecimiento_edificio_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit" class="boton_rojo">Eliminar</button>
        </form>
    </li>
</ul>

{% endblock %}