{% extends 'BackendBundle::layout.html.twig' %}


{% block main_content %}
    {{ parent() }}

    <div class="titulo">Crear nueva frase</div>
    <br />

    <form class="well" action="{{ path('backend_frase_create') }}" method="post" {{ form_enctype(form) }}>
        <div class="ancho_10">
            {{ form_widget(form) }}
        </div>
        <p>
            <button type="submit" >Crear</button>
            <a class="separador" href="{{ path('backend_frase') }}">
                Volver a la lista
            </a>
            <a class="separador" href="{{ path('backend_frase_new') }}">
                Nueva frase
            </a>
        </p>
    </form>

    <br />
{% endblock %}