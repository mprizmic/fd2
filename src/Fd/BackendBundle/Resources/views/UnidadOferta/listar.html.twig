{# parametros:
lista de unidad_oferta 
#}

{% for entity in entities %}
    <span>
        <a href="{{ path('backend_unidadoferta_edit', { 'id': entity.id }) }}">
            {% if entity is instancia_de_carrera %}
                {{ entity.identificacion }}
            {% else %}
                {{ entity }}
            {% endif %}
        </a><br />
    </span>
{% else %}
    <span class="separador resaltado mediana">
        No hay datos
    </span>
{% endfor %}