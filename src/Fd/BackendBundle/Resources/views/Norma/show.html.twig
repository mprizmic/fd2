{% extends 'BackendBundle::layout.html.twig' %}
 
{% block title %}
{{ parent() }} - Norma
{% endblock %}
 
{% block main_content %}

<h1>Norma</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Numero</th>
            <td>{{ entity.numero }}</td>
        </tr>
        <tr>
            <th>Descripcion</th>
            <td>{{ entity.descripcion }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('backend_norma_buscar') }}">
            Volver a la lista
        </a>
    </li>
    <li>
        <a href="{{ path('backend_norma_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
</ul>

{% endblock %}