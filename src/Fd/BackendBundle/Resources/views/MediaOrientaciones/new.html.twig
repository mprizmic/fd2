{% extends 'BackendBundle::layout.html.twig' %}

{% block title %}
    {{ parent() }} - Crear orientación de la NES
{% endblock %}

{% block main_content %}

    <h1>Crear registro de orientación de la NES</h1>

    <form action="{{ path('backend.mediaorientaciones.create') }}" method="post" {{ form_enctype(form) }}>
        <div class="ancho_10">
            {{ form_widget(form) }}
        </div>
        <p class="bloque">
            <button type="submit">Crear</button>
            <a class="separador" href="{{ path('backend.mediaorientaciones.buscar') }}">Volver</a>
        </p>
    </form>


{% endblock %}
