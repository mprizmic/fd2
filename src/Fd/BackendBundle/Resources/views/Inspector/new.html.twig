{% extends 'BackendBundle::layout.html.twig' %}

{% block title %}
    {{ parent() }} - Crear Inspector
{% endblock %}

{% block main_content %}

    <h1>Crear registro de nuevo Inspector de infraestructura</h1>

    <form action="{{ path('backend_inspector_create') }}" method="post" {{ form_enctype(form) }}>
        <div class="ancho_10">
            {{ form_widget(form) }}
        </div>
        <div class="bloque">
            <button class="separador" type="submit">Crear</button>
            <a class="separador" href="{{ path('backend_inspector') }}">
                Volver a la lista
            </a>
        </div>
    </form>

    <br />
    <br />
{% endblock %}
