{% extends 'BackendBundle::layout.html.twig' %}

{% block title %}
    {{ parent() }} - Crear DomicilioLocalizacion
{% endblock %}

{% block main_content %}
    {{ parent() }}

    <div class="titulo">Crear un nuevo domicilio de un nivel de un establecimiento</div>
    <br />

    <form action="{{ path('backend_domicilio_localizacion_create') }}" method="post" {{ form_enctype(form) }}>
        <div class="ancho_10">
        {{ form_widget(form) }}
        </div>
        <p>
            <button type="submit">Crear</button>
            <a class="separador" href="{{ path('backend_domicilio_localizacion') }}">
                Volver a la lista
            </a>
        </p>
    </form>

    <br />
    <br />
{% endblock %}
