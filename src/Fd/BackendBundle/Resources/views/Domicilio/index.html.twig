{% extends 'BackendBundle::layout.html.twig' %}
 
{% block title %}
{{ parent() }} - Lista de Domicilio
{% endblock %}
 
{% block main_content %}

<h1>Lista de Domicilio</h1>

<ul>
    <li>
        <a href="{{ path('backend_domicilio_new') }}">
            Crear nuevo registro
        </a>
    </li>
</ul>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Calle</th>
            <th>Altura</th>
            <th>C. Postal</th>
            <th>Principal</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('backend_domicilio_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.calle }}</td>
            <td>{{ entity.altura }}</td>
            <td>{{ entity.cPostal }}</td>
            <td>{{ entity.principal }}</td>
            <td><a href="{{ path('backend_domicilio_edit', { 'id': entity.id }) }}">Editar</a>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{% endblock %}
