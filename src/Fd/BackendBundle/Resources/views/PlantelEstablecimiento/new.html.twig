{% extends 'BackendBundle::layout.html.twig' %}

{% block main_content %}
    {{ parent() }}
    
    <div class="titulo">Crear nuevo cargo de una dependencia del establecimiento</div>

    <form action="{{ path('backend.plantelestablecimiento.create') }}" method="post" {{ form_enctype(form) }}>
        <div class="ancho_10">
            {{ form_widget(form) }}
        </div>
        <p class="separador">
            <button type="submit">Crear</button>
            <a class="separador" href="{{ path('backend.plantelestablecimiento.buscar') }}">Volver a la lista</a>
        </p>
    </form>


    <br />
    <br />

{% endblock %}