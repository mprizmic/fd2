{% extends 'BackendBundle::layout.html.twig' %}

{% block title %}
    {{ parent() }} - Crear Autoridad
{% endblock %}

{% block main_content %}

    <h1>Crear registro de Autoridad </h1>

    <form action="{{ path('backend_autoridad_create') }}" method="post" {{ form_enctype(form) }}>
        <div class="ancho_10">
            {{ form_widget(form) }}
        </div>
        <span class="separador">
            <button type="submit">Crear</button>
        </span>
    </form>

    <div class="float_derecha">
        <a href="{{ path('backend_autoridad_buscar') }}">
            Volver
        </a>
    </div>

    <br />
    <br />

{% endblock %}
