{% extends 'BackendBundle::layout.html.twig' %}
 
{% block title %}
{{ parent() }} - Crear Glosario
{% endblock %}
 
{% block main_content %}
{{ parent() }}

<h1>Crear registro de Glosario </h1>

<form action="{{ path('backend_glosario_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Crear</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('backend_glosario') }}">
            Volver a la lista
        </a>
    </li>
</ul>

{% endblock %}
